Compute_Month_Probability <- function(p,prior_mp,i){
prob_table <- p %>% group_by(bigram) %>%
summarize(occurrence=length(bigram),probability_occur = length(bigram)/nrow(p),log_probability_occur = log(probability_occur) )
assign(paste("probability",i,sep=""),prob_table,envir=.GlobalEnv)
}
